{
    "version": "0.2.0",
    "inputs": [
        {
            "id": "trace",
            "type": "pickString",
            "description": "which trace to run",
            "options": [
                "traces/amptjp-bal.rep",
                "traces/amptjp.rep",
                "traces/binary2-bal.rep",
                "traces/binary2.rep",
                "traces/binary-bal.rep",
                "traces/binary.rep",
                "traces/cccp-bal.rep",
                "traces/cccp.rep",
                "traces/coalescing-bal.rep",
                "traces/coalescing.rep",
                "traces/cp-decl-bal.rep",
                "traces/cp-decl.rep",
                "traces/expr-bal.rep",
                "traces/expr.rep",
                "traces/random2-bal.rep",
                "traces/random2.rep",
                "traces/random-bal.rep",
                "traces/random.rep",
                "traces/realloc2-bal.rep",
                "traces/realloc2.rep",
                "traces/realloc-bal.rep",
                "traces/realloc.rep",
